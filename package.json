{
  "name": "@mmanela/marked-terminal",
  "version": "7.3.4",
  "description": "A custom render for marked to output to the Terminal",
  "main": "./dist/index.js",
  "browser": "./dist/index.js",
  "exports": {
    "node": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "default": "./dist/index.js"
  },
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsc",
    "prepack": "npm run build",
    "test": "cross-env FORCE_HYPERLINK=0 mocha tests/markedTerminal-*.js tests/e2e.js --reporter spec"
  },
  "files": [
    "dist",
    "supports-color.js",
    "supports-hyperlinks.js"
  ],
  "types": "dist/index.d.ts",
  "keywords": [
    "marked",
    "render",
    "terminal",
    "markdown",
    "markdown-to-terminal"
  ],
  "author": "Mikael Brevik",
  "license": "MIT",
  "peerDependencies": {
    "marked": ">=1 <16"
  },
  "dependencies": {
    "ansi-escapes": "^7.0.0",
    "ansi-regex": "^6.1.0",
    "chalk": "^5.4.1",
    "cli-highlight": "^2.1.11",
    "cli-table3": "^0.6.5",
    "node-emoji": "^2.2.0",
		"has-flag": "^5.0.1"
  },
  "directories": {
    "example": "example"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "cross-env": "^7.0.3",
    "marked": "^15.0.0",
    "mocha": "^11.0.1",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mikaelbr/marked-terminal.git"
  },
  "bugs": {
    "url": "https://github.com/mikaelbr/marked-terminal/issues"
  },
  "homepage": "https://github.com/mikaelbr/marked-terminal"
}
